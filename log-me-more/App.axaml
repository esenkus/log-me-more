<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:log_me_more"
             x:Class="log_me_more.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark" />
    </Application.Styles>
    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryBackgroundColor">#073042</SolidColorBrush>
        <SolidColorBrush x:Key="SecondaryBackgroundColor">#1c6a60</SolidColorBrush>

    </Application.Resources>
</Application>